     Judgment Interface Parameter [ | | ppHaskell USE {>-<} {empty}: {PP_Doc} ]
     
--ATTR Judgment Parameters Parameter [ | | inParams : Parameters outParams : Parameters ]

SEM Implementation
  | Implementation        lhs.ppHaskell = @rules.ppHaskell
  
SEM Rule
  | Rule                  lhs.ppHaskell = text @name
                                          >-< indent 2 (text "= let")
                                          >-< indent 6 (text "result = undefined")
                                          >-< indent 4 (text "in" >#< text "result")

                                                                                    
                                          
--SEM Parameter
--  | Parameter             lhs.ppHaskell = undefined 
                          
{
isIn :: Direction ->  Bool
isIn (In)    = True
isIn (InOut) = True
isIn (Out)   = False

isOut :: Direction -> Bool
isOut (In)    = False
isOut (InOut) = True
isOut (Out)   = True
}   