%format Sum  = "\Typeid{Sum}"
%format Prod = "\Typeid{Prod}"
%format :*:  = "\times "
%format Con = "\Typeid{Con}"
%format ConC = "\Conid{Con}"
%format Lab = "\Typeid{Lab}"
%format LabC = "\Conid{Lab}"

%format ConDescr = "\Typeid{ConDescr}"
%format LabDescr = "\Typeid{LabDescr}"

%format EP   = "\Typeid{EP}"
%format Ep   = "\Conid{EP}"

%format ForallS = "\Typeid{Forall}_{" * "}"
%format FAS = "\Conid{Forall}_{" * "}"

%format Add = "\Classid{Add}"
%format Size = "\Classid{Size}"

%format Star(n) = * "^{" n "}"
%endif
