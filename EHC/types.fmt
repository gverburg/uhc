%if style == poly
\newcommand{\Typeid}{\mathrm}
\newcommand{\Classid}{\mathsf}
%format Bool = "\Typeid{Bool}"
%format Int  = "\Typeid{Int}"
%format Float = "\Typeid{Float}"
%format Char = "\Typeid{Char}"
%format List = "\Typeid{List}"
%format ListF = "\Typeid{ListF}"
%format Maybe = "\Typeid{Maybe}"
%format Either = "\Typeid{Either}"
%format IO = "\Typeid{IO}"
%format Tree = "\Typeid{Tree}"
%format Nat = "\Typeid{Nat}"
%format Rose = "\Typeid{Rose}"
%format Bush = "\Typeid{Bush}"
%format Seq = "\Typeid{Seq}"
%format Pair = ( "\mathbin{,}" )
%format CprResult = "\Typeid{CprResult}"
%format Cardinality = "\Typeid{Cardinality}"
%format Frequency = "\Typeid{Frequency}"
%format Range = "\Typeid{Range}"
%format Position = "\Typeid{Position}"
