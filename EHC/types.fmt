%format PositionC = "\Conid{Position}"
%format String = "\Typeid{String}"
%format Bit = "\Typeid{Bit}"
%format OBit = "\mathtt{0}"
%format IBit = "\mathtt{1}"
%format Motion = "\Typeid{Motion}"
%format IntStruct = "\Typeid{IntStruct}"
%format IntCharStruct = "\Typeid{IntCharStruct}"
%format Fix = "\Typeid{Fix}"
%format Id = "\Typeid{Id}"
%format Type = "\Typeid{Type}"
%format ZeroC = "\Conid{Zero}"
%format SuccC = "\Conid{Succ}"
%format InC = "\Conid{In}"
%format NatF = "\Typeid{NatF}"
%format Zipper = "\Typeid{Zipper}"
%format Loc = "\Typeid{Loc}"
%format Context = "\Typeid{Context}"
%format Ctx = "\Typeid{Ctx}"
%format CtxF = "\Typeid{CtxF}"
%format ID = "\Typeid{ID}"
%format FMap = "\Typeid{FMap}"
%format FMap_Sum = "\Typeid{FMapSum}"
%format FMap_Prod = "\Typeid{FMapProd}"
%format IntMap = "\Typeid{IntMap}"
%format CharMap = "\Typeid{CharMap}"
%format Direction = "\Typeid{Direction}"
%format Set = "\Typeid{Set}"
%format SetC = "\Conid{Set}"
%format Expr = "\Typeid{Expr}"
%format Var = "\Typeid{Var}"
%format VarC = "\Conid{Var}"
%format Type = "\Typeid{Type}"
%format LetC = "\Conid{Let}"
%format AlgebraOf = "\Typeid{AlgebraOf}"

%format Zero = "\Typeid{Zero}"
%format ParZero = "\Typeid{ParZero}"
%format Unit = "\Typeid{Unit}"
