SEM TyExpr
  | Quant       loc         .   ty                  =   Ty_Quant @qu @tv @tyExpr.ty
%%]

%%[5
ATTR TyExprs [ | | tyL: TyL ]

SEM TyExprs
  | Nil         lhs         .   tyL                 =   []
  | Cons        lhs         .   tyL                 =   @hd.ty : @tl.tyL
%%]

%%[5
ATTR TyVar [ | | ty: Ty ]
ATTR TyVars [ | | tyL: TyL ]

SEM TyVar
  | Var         lhs         .   ty                  =   tgiTy @tgi_

SEM TyVars
