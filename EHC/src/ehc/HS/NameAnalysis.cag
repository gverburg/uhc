%%[1 hs
mkDefGam' :: NmLev -> Range -> (IdOcc -> IdAspect) -> [IdOcc] -> IdDefOccGam
mkDefGam' l r mka os
  = assocLToGam [ (o,mkIdDefOcc o (mka o) l r) | o <- os ]

mkDefGam :: NmLev -> Range -> IdAspect -> [IdOcc] -> IdDefOccGam
mkDefGam l r a os
  = mkDefGam' l r (const a) os
%%]

%%[3 hs
tvarL2IdDefOccGam :: NmLev -> Range -> [HsName] -> IdDefOccGam
tvarL2IdDefOccGam l r ns
  = mkDefGam l r IdAsp_Type_Var [ IdOcc n IdOcc_Type | n <- ns ]

tvarS2IdDefOccGam :: NmLev -> Range -> Set.Set HsName -> IdDefOccGam
tvarS2IdDefOccGam l r ns
  = tvarL2IdDefOccGam l r (Set.toList ns)
%%]

