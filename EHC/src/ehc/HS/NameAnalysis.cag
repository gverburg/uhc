%%[1.mkUseOccGam hs
mkUseOccGam :: IdDefOccGam -> [HsName] -> IdOccKind -> IdAspect -> Range -> (HsName,[HsName],IdUseOccGam,[Err])
mkUseOccGam _ [] _ _ _
  = (undefined,[],emptyGam,[])
mkUseOccGam idGam names@(fstName:_) kind asp rng
  = (fstName,names,gamUnions [ mkUse idGam n kind asp rng | n <- names ],[])
  where mkUse idGam name kind asp rng
          = idUseOccGam
          where idOccUse    = IdOcc name kind
                mbDef       = gamLookup idOccUse idGam
                idUseOccGam = gamUnit idOccUse (IdUseOcc idOccUse asp rng (doccForUse mbDef))
%%]

%%[12 -1.mkUseOccGam hs
mkUseOccGam :: IdDefOccGam -> [HsName] -> IdOccKind -> IdAspect -> Range -> (HsName,[HsName],IdUseOccGam,[Err])
mkUseOccGam _ [] _ _ _
  = (undefined,[],emptyGam,[])
mkUseOccGam idGam names kind asp rng
  = (n, ns, gamUnions g, concat e)
  where (ns@(n:_),g,e) = unzip3 [ mkUse idGam n kind asp rng | n <- names ]
        mkUse idGam name kind asp rng
          = (nmOfDef,idUseOccGam,errs)
          where idOccUse    = IdOcc name kind
%%[[12
                mbDef       = gamLookup idOccUse idGam
%%][99
                mbDef       = case gamLookup idOccUse idGam of
                                Nothing | hsnIsInPrelude name -> Just (emptyIdDefOcc {doccOcc = idOccUse})
                                md                            -> md
%%]]
                nmOfDef     = case mbDef of
                                Just d -> ioccNm $ doccOcc d
                                _      -> name
                occOfDef    = idOccUse {ioccNm = nmOfDef}
                idUseOccGam = gamUnit occOfDef (IdUseOcc occOfDef asp rng (doccForUse mbDef))
                errs        = case mbDef of
                                Just d | isJust (doccNmAlts d) && length alts > 1
                                  -> [Err_AmbiguousNameRef name alts]
                                  where alts = fromJust (doccNmAlts d)
                                _ -> []
%%]

