layer Equational
   interface Expr (e)
      params
         in    valGam    : ValGam
         in    tyGam     : TyGam
         in    kiGam     : KiGam
         inout ty        : Ty
      pattern "kiGam ; tyGam ; valGam :- e : ty"
      
implementation of Equational
  rule fitsIn implements Expr
    pre
    post R : Expr = kiGam ; tyGam ; valGam :- int : tyInt